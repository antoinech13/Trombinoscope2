<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_file"
    app:startDestination="@id/logginFragment">
    <fragment
        android:id="@+id/trombinoscopes3"
        android:name="com.example.trombinoscope.fragments.trombinoscopes"
        android:label="trombinoscopes_fragment"
        tools:layout="@layout/trombinoscopes_fragment" >
        <action
            android:id="@+id/action_trombinoscopes3_to_addTrombinoscopes"
            app:destination="@id/addTrombinoscopes" />
        <action
            android:id="@+id/action_trombinoscopes3_to_editTrombi"
            app:destination="@id/editTrombi" />
    </fragment>
    <fragment
        android:id="@+id/addTrombinoscopes"
        android:name="com.example.trombinoscope.fragments.AddTrombinoscopes"
        android:label="fragment_add_trombinoscopes"
        tools:layout="@layout/fragment_add_trombinoscopes" >
        <action
            android:id="@+id/action_addTrombinoscopes_to_trombinoscopes3"
            app:destination="@id/trombinoscopes3" />
        <action
            android:id="@+id/action_addTrombinoscopes_to_addUniversityFragment"
            app:destination="@id/addUniversityFragment" />
    </fragment>
    <fragment
        android:id="@+id/addToTrombi"
        android:name="com.example.trombinoscope.fragments.AddToTrombi"
        android:label="fragment_add_to_trombi"
        tools:layout="@layout/fragment_add_to_trombi" >
        <action
            android:id="@+id/action_addToTrombi_to_editTrombi"
            app:destination="@id/editTrombi" />
        <action
            android:id="@+id/action_addToTrombi_to_camFrag2"
            app:destination="@id/camFrag2" />
    </fragment>
    <fragment
        android:id="@+id/editTrombi"
        android:name="com.example.trombinoscope.fragments.EditTrombi"
        android:label="fragment_edit_trombi"
        tools:layout="@layout/fragment_edit_trombi" >
        <action
            android:id="@+id/action_editTrombi_to_addToTrombi"
            app:destination="@id/addToTrombi" />
    </fragment>
    <fragment
        android:id="@+id/logginFragment"
        android:name="com.example.trombinoscope.fragments.LogginFragment"
        android:label="fragment_loggin"
        tools:layout="@layout/fragment_loggin" >
        <action
            android:id="@+id/action_logginFragment_to_trombinoscopes3"
            app:destination="@id/trombinoscopes3" />
        <action
            android:id="@+id/action_logginFragment_to_signIn"
            app:destination="@id/signIn" />
        <action
            android:id="@+id/action_logginFragment_to_forgotPw"
            app:destination="@id/forgotPw" />
    </fragment>
    <fragment
        android:id="@+id/addUniversityFragment"
        android:name="com.example.trombinoscope.fragments.AddUniversityFragment"
        android:label="fragment_add_university"
        tools:layout="@layout/fragment_add_university" >
        <action
            android:id="@+id/action_addUniversityFragment_to_addTrombinoscopes"
            app:destination="@id/addTrombinoscopes" />
    </fragment>
    <fragment
        android:id="@+id/signIn"
        android:name="com.example.trombinoscope.fragments.SignIn"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >
        <action
            android:id="@+id/action_signIn_to_logginFragment"
            app:destination="@id/logginFragment" />
        <action
            android:id="@+id/action_signIn_to_userCondition"
            app:destination="@id/userCondition" />
    </fragment>
    <fragment
        android:id="@+id/userCondition"
        android:name="com.example.trombinoscope.fragments.UserCondition"
        android:label="fragment_user_condition"
        tools:layout="@layout/fragment_user_condition" />
    <fragment
        android:id="@+id/camFrag2"
        android:name="com.example.trombinoscope.fragments.CamFrag"
        android:label="fragment_cam"
        tools:layout="@layout/fragment_cam" >
        <action
            android:id="@+id/action_camFrag2_to_addToTrombi"
            app:destination="@id/addToTrombi" />
    </fragment>

    <fragment
        android:id="@+id/forgotPw"
        android:name="com.example.trombinoscope.fragments.ForgotPw"
        android:label="fragment_forgot_pw"
        tools:layout="@layout/fragment_forgot_pw" >
        <action
            android:id="@+id/action_forgotPw_to_verifCodeRecupPw"
            app:destination="@id/verifCodeRecupPw" />
    </fragment>
    <fragment
        android:id="@+id/pwRecup"
        android:name="com.example.trombinoscope.fragments.PwRecup"
        android:label="fragment_pw_recup"
        tools:layout="@layout/fragment_pw_recup" >
        <action
            android:id="@+id/action_pwRecup_to_logginFragment"
            app:destination="@id/logginFragment" />
    </fragment>
    <fragment
        android:id="@+id/verifCodeRecupPw"
        android:name="com.example.trombinoscope.fragments.VerifCodeRecupPw"
        android:label="fragment_verif_code_recup_pw"
        tools:layout="@layout/fragment_verif_code_recup_pw" >
        <action
            android:id="@+id/action_verifCodeRecupPw_to_pwRecup"
            app:destination="@id/pwRecup" />
    </fragment>
</navigation>